            <script>
              function selectLogin() {  

                    $("#btnPaciente").click(function() {                      
                      $("#login-form-paciente").show();
                      $("#login-form-medico").hide();                                   
                      $("#login-form-posto").hide();        
                    });

                     $("#btnPosto").click(function() {
                      $("#login-form-posto").show();
                      $("#login-form-medico").hide();                                   
                      $("#login-form-paciente").hide();                
                    });

                      $("#btnMedico").click(function() {
                       $("#login-form-medico").show();
                       $("#login-form-paciente").hide();                                   
                       $("#login-form-posto").hide();                
                    });

                    $("#login-form-medico").hide();                                   
                    $("#login-form-posto").hide();
                  }
              $(document).ready(selectLogin);
            </script>         
      
      
